########################################
# Database
########################################
# MongoDB connection string, e.g.: mongodb://127.0.0.1:27017/turn_database
MONGODB_URI=

########################################
# Auth0 configuration (fill with your tenant details)
########################################
# Base domain, e.g., your-tenant.us.auth0.com (without protocol) or full base URL if you prefer
AUTH0_DOMAIN=
# Database connection name configured in Auth0 (e.g., Username-Password-Authentication)
AUTH0_DB_CONNECTION=
# A human-friendly name for your application (optional, for logs/metadata)
AUTH0_APP_NAME=

# Public client used for non-privileged Authentication API flows (if needed)
AUTH0_AUTHN_CLIENT_ID=

# Machine-to-Machine app for Management API
AUTH0_MGMT_CLIENT_ID=
AUTH0_MGMT_CLIENT_SECRET=
# Audience for Management API, typically: https://<AUTH0_DOMAIN>/api/v2/
AUTH0_MGMT_AUDIENCE=
