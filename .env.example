########################################
# Database
########################################
# MongoDB connection string, e.g.: mongodb://127.0.0.1:27017/turn_database
MONGODB_URI=

########################################
# Auth0 configuration (fill with your tenant details)
########################################
# Base domain, e.g., your-tenant.us.auth0.com (without protocol) or full base URL if you prefer
AUTH0_DOMAIN=
# Database connection name configured in Auth0 (e.g., Username-Password-Authentication)
AUTH0_DB_CONNECTION=
# A human-friendly name for your application (optional, for logs/metadata)
AUTH0_APP_NAME=

# Public or confidential client used for Authentication API flows.
# If using Resource Owner Password (Password Grant), ensure the grant is ENABLED for this client.
AUTH0_AUTHN_CLIENT_ID=
## Optional for confidential clients (set only if your client has a secret)
AUTH0_AUTHN_CLIENT_SECRET=
## Optional audience. If you want API access tokens, set your API Identifier here (e.g., https://api.myapp.com)
AUTH0_AUTHN_AUDIENCE=
## Optional scopes for the Authentication API token. Default used by code: "openid profile email"
AUTH0_AUTHN_SCOPE=

# Machine-to-Machine app for Management API
AUTH0_MGMT_CLIENT_ID=
AUTH0_MGMT_CLIENT_SECRET=
# Audience for Management API, typically: https://<AUTH0_DOMAIN>/api/v2/
AUTH0_MGMT_AUDIENCE=
